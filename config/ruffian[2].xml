<?xml version="1.0" encoding="UTF-8"?>
<APPLICATION_SETTINGS>
<PLUGIN type="uci" ID="ruffian">
	<NAME> Ruffian </NAME>
	<EXEC os="Windows">
		<COMMAND> %dir%\ruffian.exe </COMMAND>
	</EXEC>
	<DIR> %local%/engines/ruffian </DIR>
	<LOGO> %dir%/ruffian.gif </LOGO>
	<VERSION> 1.0.1 </VERSION>
	<AUTHOR> Perola Valfridsson </AUTHOR>
	<OPTIONS>
		<OPTION name="Hash"> 32 </OPTION>
		<OPTION name="MultiPV"> 6 </OPTION>
		<OPTION name="NalimovPath"> c:\chess\tb </OPTION>
		<OPTION name="NalimovCache"> 1 </OPTION>
		<OPTION name="Ponder"> true </OPTION>
		<OPTION name="OwnBook"> true </OPTION>
	</OPTIONS>
	<STARTUP/>
	<ARGS/>
	<EXEC os="Linux_i386">
		<COMMAND/>
	</EXEC>
</PLUGIN></APPLICATION_SETTINGS>